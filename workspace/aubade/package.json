{
	"name": "aubade",
	"version": "0.9.1",
	"description": "filesystem-based content processor",
	"repository": "github:ignatiusmb/aubade",
	"homepage": "https://aubade.mauss.dev",
	"author": "Ignatius Bagus",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"test": "pnpm run \"/^test:.*/\"",
		"test:core": "uvu src \"(spec\\.js)\"",
		"test:apps": "uvu test/apps",
		"test:types": "tsc -p src/tsconfig.test.json",
		"build:types": "tsm scripts/compile-types.ts",
		"build:files": "tsc --project src",
		"prepublish": "pnpm run \"/^build:.*/\""
	},
	"typings": "index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./src/core/index.js"
		},
		"./artisan": {
			"types": "./index.d.ts",
			"default": "./src/artisan/index.js"
		},
		"./browser": {
			"types": "./index.d.ts",
			"default": "./src/browser/index.js"
		},
		"./compass": {
			"types": "./index.d.ts",
			"default": "./src/compass/index.js"
		},
		"./transform": {
			"types": "./index.d.ts",
			"default": "./src/transform/index.js"
		},
		"./styles/*.css": {
			"types": "./styles/index.d.ts",
			"default": "./styles/*.css"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"src/**/*.js",
		"styles/",
		"index.d.ts",
		"index.d.ts.map"
	],
	"engines": {
		"node": ">=16.13"
	},
	"keywords": [
		"filesystem",
		"markdown",
		"compiler",
		"yaml",
		"parser",
		"front-matter",
		"code-highlighter",
		"content-processor",
		"typescript"
	],
	"dependencies": {
		"@types/markdown-it": "^14.1.2",
		"markdown-it": "^14.1.0",
		"mauss": "^0.10.0",
		"shiki": "^3.6.0"
	},
	"devDependencies": {
		"dts-buddy": "^0.6.2",
		"tsm": "^2.3.0",
		"uvu": "^0.5.6"
	}
}
