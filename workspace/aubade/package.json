{
	"name": "aubade",
	"version": "0.11.5",
	"description": "markdown, orchestrated.",
	"repository": "github:ignatiusmb/aubade",
	"homepage": "https://aubade.mauss.dev",
	"author": "Ignatius Bagus",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"dev": "vitest",
		"test": "pnpm run \"/^test:.*/\"",
		"test:core": "vitest run src",
		"test:types": "tsc -p src/tsconfig.test.json",
		"build:types": "node scripts/dts-buddy.js",
		"build:files": "tsc --project src",
		"prepublishOnly": "pnpm run \"/^build:.*/\""
	},
	"typings": "index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./src/core/index.js"
		},
		"./artisan": {
			"types": "./index.d.ts",
			"default": "./src/artisan/index.js"
		},
		"./browser": {
			"types": "./index.d.ts",
			"default": "./src/browser/index.js"
		},
		"./conductor": {
			"types": "./index.d.ts",
			"default": "./src/conductor/index.js"
		},
		"./manifest": {
			"types": "./index.d.ts",
			"default": "./src/manifest/index.js"
		},
		"./palette": {
			"types": "./index.d.ts",
			"default": "./src/palette/index.js"
		},
		"./transform": {
			"types": "./index.d.ts",
			"default": "./src/transform/index.js"
		},
		"./styles/*.css": {
			"types": "./styles/index.d.ts",
			"default": "./styles/*.css"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"src/**/*.js",
		"styles/",
		"index.d.ts",
		"index.d.ts.map"
	],
	"engines": {
		"node": ">=16.13"
	},
	"keywords": [
		"filesystem",
		"markdown",
		"compiler",
		"yaml",
		"parser",
		"front-matter",
		"code-highlighter",
		"content-processor",
		"typescript"
	],
	"dependencies": {
		"shiki": "^3.12.0"
	},
	"devDependencies": {
		"mauss": "^0.10.7"
	}
}
